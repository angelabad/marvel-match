    "vue-gtag": "^1.16.1",
    "vue-meta": "^2.4.0",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.5.3",
    "vue-scrollto": "^2.20.0",
    "vue-social-sharing": "^2.4.7",
    "vue-template-compiler": "^2.6.14"
